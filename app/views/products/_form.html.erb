	<p>
		<%= form.label :name %><br>
		<%= form.text_field :name %>
	</p>

	<p>
		<%= form.label :description %><br>
		<%= form.text_area :description, rows: 5, cols: 50 %>
	</p>

	<p>
		<%= form.label :price_in_cents %><br>
		<%= form.number_field :price_in_cents %>
	</p>

	<p>
		<%= form.label :download_url %><br>
		<%= form.text_field :download_url %>
	</p>

	<p>
		<%= form.label :image %><br>
		<%= form.file_field :image %>
	</p>

	<%= form.submit %>