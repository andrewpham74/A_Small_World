<h1>Upload from web</h1>

<%= form_tag select_picture_pins_path, method: :get do %>
	<div class="form-group">
    	<%= label_tag :link %><br>
    	<%= text_field_tag :link, nil, class: "form-control", style: 'width: 400px' %>
  	</div>

  <div class="form-group">
    <%= submit_tag 'Submit', class: "btn btn-primary" %>
  </div>
<% end %>